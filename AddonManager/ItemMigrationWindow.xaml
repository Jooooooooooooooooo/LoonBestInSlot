<Window x:Class="AddonManager.ItemMigrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AddonManager"
        mc:Ignorable="d"
        Title="ItemMigrationWindow" Height="450" Width="800">
    <Grid>
        <Button Content="ReadSpec" HorizontalAlignment="Left" Margin="10,9,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ScrollViewer Margin="0,35,0,0" BorderBrush="Gray" BorderThickness="1">
            <TextBlock x:Name="ConsoleOut" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" />
        </ScrollViewer>
        
        <TextBox x:Name="SpecText" HorizontalAlignment="Left" Margin="81,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
